        function deleteScript()
        {
                $.ajax({
                    url: 'ScriptHubHandler.php',
                    dataType: 'json',
                    method: 'post',
                    data: {
                        scriptid: 'DFLhZ3KHWX',
                        type: 'deleteScript'
                    },
